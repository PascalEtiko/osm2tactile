<?xml version="1.0" encoding="UTF-8"?>
<Map background-color="#ffffff" srs="+proj=longlat +ellps=WGS84 +datum=WGS84 +no_defs" minimum-version="3.0.0">
  <Style name="roads-fill-dejavu">
    <Rule>
      <Filter>[highway] !=''</Filter>
      <LineSymbolizer stroke="#000000" stroke-width="3"/>
      <TextSymbolizer allow-overlap="false" face-name="DejaVu Sans Book" size="16" placement="line" dx="0.0" dy="-15.0">[name]</TextSymbolizer>
    </Rule>
  </Style>
</Map>
